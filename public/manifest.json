{
  "manifest_version": 2,
  "name": "Marksort",
  "default_locale": "en",
  "description": "A extension help you order your bookmarks easily.",
  "version": "1.0",
  "icons": {
    "16": "icon16.png",
    "48": "icon48.png",
    "128": "icon128.png"
  },
  "options_ui": {
    "page": "options.html"
  },
  "browser_action": {
    "default_icon": "icon16.png"
  },
  "background": {
    "persistent": false,
    "scripts": ["js/vendor.js", "js/background.js"]
  },
  "permissions": ["storage", "bookmarks"]
}
